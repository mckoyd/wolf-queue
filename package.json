{
  "name": "wolf-queue",
  "version": "1.0.0",
  "description": "An implementation of a queue data structure",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "start": "nodemon --exec babel-node index.js",
    "build:src": "babel src --out-dir dist",
    "build": "npm run clean && npm run build:src",
    "test": "jest --watchAll --coverage",
    "test:cov": "jest --coverage",
    "check": "prettier --check --write 'src/**/*.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mckoyd/wolf-queue.git"
  },
  "author": "Dominick McKoy <dominick.mckoy@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mckoyd/wolf-queue/issues"
  },
  "homepage": "https://github.com/mckoyd/wolf-queue#readme",
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "babel-preset-es2015": "6.24.1",
    "jest": "25.1.0",
    "nodemon": "2.0.2",
    "prettier": "1.19.1"
  }
}
